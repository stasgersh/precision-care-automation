{
  "id": "global-cteai",
  "status": "Recruiting",
  "type": "trial",
  "name": "Study to Evaluate the Safety and Tolerability of CC-94676 in Participants With Metastatic Castration-Resistant Prostate Cancer",
  "description": "The purpose of this study is to assess the safety, tolerability and preliminary efficacy of CC-94676 in men with progressive metastatic castration resistant prostate cancer.",
  "patientsFilter": {
    "filter": {
      "must": [
        {
          "attributes.gender": {
            "eq": "male"
          }
        }
      ]
    }
  },
  "primaryCompletion": null,
  "studyCompletion": "2026-12-27",
  "phase": "1",
  "link": "https://clinicaltrials.gov/study/NCT04428788?a=23",
  "variables": [
    {
      "id": "dob",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "PATIENT",
              "query": {
                "show": [
                  "attributes.dob"
                ],
                "dedup": true,
                "from": "patient"
              }
            }
          ]
        }
      },
      "display": "Patient's date of birth",
      "usingAi": false
    },
    {
      "id": "gender",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "PATIENT",
              "query": {
                "show": [
                  "attributes.gender"
                ],
                "dedup": true,
                "from": "patient"
              }
            }
          ]
        }
      },
      "display": "Patient's gender",
      "usingAi": false
    },
    {
      "id": "ecog",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "ECOG",
              "query": {
                "show": [
                  "attributes.value"
                ],
                "dedup": true,
                "from": "ecog"
              }
            }
          ]
        }
      },
      "display": "Patient's ecog",
      "usingAi": false
    },
    {
      "id": "histologically",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "pathologyReportAlias",
              "query": {
                "show": [
                  "attributes.clpFindings",
                  "attributes.code"
                ],
                "filter": {
                  "must": [
                    {
                      "should": [
                        {
                          "attributes.$clpFindings.findings.PrimaryCancer_array.Histology_Type_str": {
                            "eq": "Adenocarcinoma"
                          }
                        },
                        {
                          "attributes.$clpFindings.findings.PrimaryCancer_array.Histologic_Type_str": {
                            "eq": "Adenocarcinoma"
                          }
                        },
                        {
                          "attributes.$clpFindings.findings.PrimaryCancer_object.Histology_Type_str": {
                            "eq": "Adenocarcinoma"
                          }
                        },
                        {
                          "attributes.$clpFindings.findings.PrimaryCancer_object.Histologic_Type_str": {
                            "eq": "Adenocarcinoma"
                          }
                        }
                      ]
                    }
                  ]
                },
                "dedup": true,
                "from": "pathologyReport"
              }
            }
          ]
        }
      },
      "display": "Patient's histologic type",
      "usingAi": true
    },
    {
      "id": "comorbidity",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "ComorbidityAlias",
              "query": {
                "show": [
                  "attributes.date"
                ],
                "filter": {
                  "must": [
                    {
                      "attributes.code.$coding.display": {
                        "eq": "Thromboembolism"
                      }
                    }
                  ]
                },
                "dedup": true,
                "from": "comorbidity"
              }
            }
          ]
        }
      },
      "display": "Patient's comorbidity",
      "usingAi": false
    },
    {
      "id": "primary_cancer_type",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "primaryCancerDiseaseAlias",
              "query": {
                "from": "primaryCancerDisease"
              }
            }
          ]
        }
      },
      "display": "Patient's primary cancer types",
      "usingAi": false
    },
    {
      "id": "ADT",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "ADT",
              "query": {
                "show": [
                  "attributes.code"
                ],
                "filter": {
                  "must": [
                    {
                      "attributes.code.$coding.display": {
                        "eq": "leuprolide"
                      }
                    }
                  ]
                },
                "from": "medication",
                "limit": 1
              }
            }
          ]
        }
      },
      "display": "Patient's ADT med",
      "usingAi": false
    },
    {
      "id": "SignificantMedicalCondition",
      "definition": {
        "ai": {
          "criteria": "Any significant medical condition, such as uncontrolled infection, laboratory abnormality, or psychiatric illness"
        }
      },
      "display": "SignificantMedicalCondition",
      "usingAi": true
    },
    {
      "id": "PriorARTreatment",
      "definition": {
        "ai": {
          "criteria": "Prior treatment with an androgen receptor (AR) degrader"
        }
      },
      "display": "Prior AR degrader treatment",
      "usingAi": true
    }
  ],
  "constants": null,
  "criteria": [
    {
      "id": "criteria_male",
      "type": "inclusion",
      "title": "Gender is male",
      "allowMissingVariables": null,
      "expression": "gender",
      "subCriteria": [
        {
          "id": "gender",
          "title": "Gender is male",
          "description": "Gender is male",
          "expression": "variables.gender.value[].attributes.gender|[0]== `male`",
          "explainability": {
            "displayValue": "variables.gender.value[].attributes.gender|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_age",
      "type": "inclusion",
      "title": "Age >= 18 years",
      "allowMissingVariables": [
        "dob"
      ],
      "expression": "isAgeGt18",
      "subCriteria": [
        {
          "id": "isAgeGt18",
          "title": "Age is greater than 18",
          "description": "Age is greater than 18",
          "expression": "variables.dob.value[].attributes.dob|abs_date_diff([0], `2024-08-07`,`years`,`int`) >= `18`",
          "explainability": {
            "displayValue": "variables.dob.value[].attributes.dob|abs_date_diff([0], `2024-08-07`,`years`,`int`)"
          }
        }
      ]
    },
    {
      "id": "ECOG_value",
      "type": "inclusion",
      "title": "Eastern Cooperative Oncology Group (ECOG) performance status (PS) 0 or 1",
      "allowMissingVariables": [
        "ecog"
      ],
      "expression": "EcogStatus",
      "subCriteria": [
        {
          "id": "EcogStatus",
          "title": "ECOG status is 0 or 1",
          "description": "ECOG status is 0 or 1",
          "expression": "variables.ecog.value[].attributes.value|[0]<=`1`",
          "explainability": {
            "displayValue": "variables.ecog.value[].attributes.value|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_histologically",
      "type": "inclusion",
      "title": "must have histologically or cytologically confirmed adenocarcinoma of the prostate",
      "allowMissingVariables": [
        "histologically"
      ],
      "expression": "isProstaticAdenocarcinoma",
      "subCriteria": [
        {
          "id": "isProstaticAdenocarcinoma",
          "title": "histologically type is prostatic adenocarcinoma",
          "description": "histologically type is prostatic adenocarcinoma",
          "expression": "variables.histologically.value[].attributes.clpFindings[].findings|[].PrimaryCancer_object.Histology_Type_str || [].PrimaryCancer_array[].Histologic_Type_str || [].PrimaryCancer_object.Histology_Type_str || [].PrimaryCancer_object.Histologic_Type_str |length(@)>`0`",
          "explainability": {
            "displayValue": "variables.histologically.value[].attributes.clpFindings[].findings|[].PrimaryCancer_object.Histology_Type_str || [].PrimaryCancer_array[].Histologic_Type_str || [].PrimaryCancer_object.Histology_Type_str || [].PrimaryCancer_object.Histologic_Type_str |[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_only_prostate_primary_cancer",
      "type": "exclusion",
      "title": "Concurrent Malignancy (present during screening) requiring treatment or history of prior malignancy active within 1 year prior to the first dose of IP",
      "allowMissingVariables": [
        "primary_cancer_type"
      ],
      "expression": "OnlyPrimaryProstateCancer",
      "subCriteria": [
        {
          "id": "OnlyPrimaryProstateCancer",
          "title": "Patient  doesn't have additional primary cancer diseases",
          "description": "Patient  doesn't have additional primary cancer diseases",
          "expression": "variables.primary_cancer_type.value[].attributes.code.coding[]|length([?! contains(display, `prostate`)])==`0`",
          "explainability": {
            "displayValue": "variables.primary_cancer_type.value[].attributes.code.coding[].display|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_Comorbidity",
      "type": "exclusion",
      "title": "Clinically significant venous thromboembolism within 3 months prior to the first dose of IP",
      "allowMissingVariables": null,
      "expression": "notThromboembolism",
      "subCriteria": [
        {
          "id": "notThromboembolism",
          "title": "No clinically significant venous thromboembolism",
          "description": "No clinically significant venous thromboembolism",
          "expression": "variables.comorbidity.value[]|[-1:].attributes.date|[0]| @ == `null` || abs_date_diff(@, `2024-09-04`,`years`)>`0` || (abs_date_diff(@, `2024-09-04`,`years`)==`0` && abs_date_diff(@, `2024-09-04`,`months`)>`3`)",
          "explainability": {
            "displayValue": "variables.comorbidity.value[]|[-1:].attributes.date|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_ADT",
      "type": "inclusion",
      "title": "Progressed on androgen deprivation therapy (ADT) and at least one prior secondary hormonal therapy approved for castration-resistant prostate cancer (CRPC)",
      "allowMissingVariables": [
        "ADT"
      ],
      "expression": "ADT",
      "subCriteria": [
        {
          "id": "ADT",
          "title": "Progressed on androgen deprivation therapy",
          "description": "Progressed on androgen deprivation therapy",
          "expression": "variables.ADT.value[].attributes.code.coding[]|length(@)>`0`",
          "explainability": {
            "displayValue": "variables.ADT.value[].attributes.code.coding[].display|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_SignificantMedicalCondition",
      "type": "exclusion",
      "title": "Any significant medical condition, such as uncontrolled infection, laboratory abnormality, or psychiatric illness",
      "allowMissingVariables": null,
      "expression": "SignificantMedicalCondition",
      "subCriteria": [
        {
          "id": "SignificantMedicalCondition",
          "title": "Any significant medical condition, such as uncontrolled infection, laboratory abnormality, or psychiatric illness",
          "description": "Any significant medical condition, such as uncontrolled infection, laboratory abnormality, or psychiatric illness",
          "expression": "variables.SignificantMedicalCondition.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.SignificantMedicalCondition.value.reasoning.join(' ', @)"
          }
        }
      ]
    },
    {
      "id": "criteria_PriorARTreatment",
      "type": "exclusion",
      "title": "Prior treatment with an androgen receptor (AR) degrader",
      "allowMissingVariables": [
        "PriorARTreatment"
      ],
      "expression": "PriorARTreatment",
      "subCriteria": [
        {
          "id": "PriorARTreatment",
          "title": "Prior treatment with an androgen receptor (AR) degrader",
          "description": "Prior treatment with an androgen receptor (AR) degrader",
          "expression": "variables.PriorARTreatment.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.PriorARTreatment.value.reasoning.join(' ', @)"
          }
        }
      ]
    }
  ]
}