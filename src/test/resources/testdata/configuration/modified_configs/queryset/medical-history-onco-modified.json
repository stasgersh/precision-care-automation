{
  "id": "medicalHistory.onco.general",
  "items": [
    [
      {
        "key": "completedMedications",
        "query": {
          "from": "medication",
          "dedup": true,
          "size": 1000,
          "filter": {
            "must": [
              {
                "attributes.status": {
                  "eq": "completed"
                }
              },
              {
                "attributes.intent": {
                  "neq": "proposal"
                }
              },
              {
                "should": [
                  {
                    "date": {
                      "range": {
                        "lte": "now/d"
                      }
                    }
                  },
                  {
                    "date": {
                      "eq": null
                    }
                  }
                ]
              }
            ]
          }
        }
      },
      {
        "key": "activeMedications",
        "query": {
          "from": "medication",
          "dedup": true,
          "size": 1000,
          "filter": {
            "must": [
              {
                "attributes.status": {
                  "eq": "active"
                }
              },
              {
                "attributes.intent": {
                  "neq": "proposal"
                }
              },
              {
                "should": [
                  {
                    "date": {
                      "range": {
                        "lte": "now/d"
                      }
                    }
                  },
                  {
                    "date": {
                      "eq": null
                    }
                  }
                ]
              }
            ]
          }
        }
      }
    ],
    [
      {
        "key": "allergies",
        "query": {
          "from": "allergy",
          "filter": {
            "must": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              }
            ]
          },
          "dedup": true
        }
      },
      {
        "key": "surgicalProcedures",
        "query": {
          "from": "surgicalProcedure",
          "filter": {
            "must": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              },
              {
                "should": [
                  {
                    "attributes.status": {
                      "eq": "in-progress | completed"
                    }
                  },
                  {
                    "attributes.status": {
                      "eq": null
                    }
                  }
                ]
              }
            ]
          },
          "dedup": true
        }
      },
      {
        "key": "familyMemberHistories",
        "query": {
          "from": "familyMemberHistory",
          "filter": {
            "should": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              },
              {
                "date": {
                  "eq": null
                }
              }
            ]
          },
          "dedup": true
        }
      },
      {
        "key": "socialHistories",
        "query": {
          "from": "socialHistory",
          "filter": {
            "must": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              },
              {
                "should": [
                  {
                    "attributes.status": {
                      "eq": "final | amended | corrected"
                    }
                  },
                  {
                    "attributes.status": {
                      "eq": null
                    }
                  }
                ]
              }
            ]
          },
          "dedup": true
        }
      },
      {
        "key": "patient",
        "query": {
          "from": "patient",
          "filter": {
            "must": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              }
            ]
          },
          "dedup": true,
          "limit": 1
        }
      }
    ]
  ]
}