{
  "configType": "entity",
  "classId": "radiotherapyCourseSummary",
  "definitions": [
    {
      "resourceType": "Procedure",
      "filters": [
        {
          "type": "terminology",
          "conditions": [
            {
              "path": "$.category",
              "include": "radiotherapy-procedure-category-vs"
            }
          ]
        },
        {
          "type": "terminology",
          "conditions": [
            {
              "path": "$.code",
              "include": "radiotherapy-procedure-vs"
            }
          ]
        }
      ],
      "date": [
        {
          "source": {
            "path": "$.performedDateTime"
          }
        },
        {
          "source": {
            "path": "$.performedPeriod"
          },
          "processor": "periodDateText"
        }
      ],
      "attributes": [
        {
          "key": "status",
          "valueExtraction": [
            {
              "source": {
                "path": "$.status"
              }
            }
          ]
        },
        {
          "key": "statusReason",
          "valueExtraction": [
            {
              "source": {
                "path": "$.statusReason"
              }
            }
          ]
        },
        {
          "key": "category",
          "valueExtraction": [
            {
              "source": {
                "path": "$.category"
              }
            }
          ]
        },
        {
          "key": "code",
          "valueExtraction": [
            {
              "source": {
                "path": "$.code"
              }
            }
          ]
        },
        {
          "key": "performedPeriod",
          "valueExtraction": [
            {
              "source": {
                "path": "$.performedPeriod"
              }
            }
          ]
        },
        {
          "key": "numberOfSessions",
          "valueExtraction": [
            {
              "source": {
                "path": "$.extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-sessions' & @.valueUnsignedInt)].valueUnsignedInt"
              }
            }
          ]
        },
        {
          "key": "intent",
          "valueExtraction": [
            {
              "source": {
                "path": "$.extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-procedure-intent' & @.valueCodeableConcept)].valueCodeableConcept"
              }
            }
          ]
        },
        {
          "key": "modalities",
          "valueExtraction": [
            {
              "source": {
                "path": "$.extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-modality-and-technique')].extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-modality')].valueCodeableConcept"
              }
            }
          ]
        },
        {
          "key": "techniques",
          "valueExtraction": [
            {
              "source": {
                "path": "$.extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-modality-and-technique')].extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-technique')].valueCodeableConcept"
              }
            }
          ]
        },
        {
          "key": "phases",
          "required": true,
          "valueExtraction": [
            {
              "source": {
                "path": "$.extension[?(@.url == 'http://hl7.org/fhir/us/mcode/StructureDefinition/mcode-radiotherapy-dose-delivered-to-volume')]",
                "attributes": [
                  {
                    "key": "bodyStructure",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.extension[?(@.url == 'volume')].valueReference",
                          "refersTo": {
                            "resourceType": "BodyStructure",
                            "attributes": [
                              {
                                "key": "location",
                                "valueExtraction": [
                                  {
                                    "source": {
                                      "path": "$.location"
                                    }
                                  }
                                ]
                              },
                              {
                                "key": "locationQualifier",
                                "valueExtraction": [
                                  {
                                    "source": {
                                      "path": "$.locationQualifier"
                                    }
                                  }
                                ]
                              },
                              {
                                "key": "description",
                                "valueExtraction": [
                                  {
                                    "source": {
                                      "path": "$.description"
                                    }
                                  }
                                ]
                              }
                            ]
                          }
                        }
                      }
                    ]
                  },
                  {
                    "key": "numberOfFractions",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.extension[?(@.url == 'fractionsDelivered')].valueUnsignedInt"
                        }
                      }
                    ]
                  },
                  {
                    "key": "totalDose",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.extension[?(@.url == 'totalDoseDelivered')].valueQuantity"
                        }
                      }
                    ]
                  }
                ]
              }
            }
          ]
        },
        {
          "key": "bodySite",
          "valueExtraction": [
            {
              "source": {
                "path": "$.bodySite"
              }
            }
          ]
        },
        {
          "key": "outcome",
          "valueExtraction": [
            {
              "source": {
                "path": "$.outcome"
              }
            }
          ]
        },
        {
          "key": "note",
          "valueExtraction": [
            {
              "source": {
                "path": "$.note[*]",
                "attributes": [
                  {
                    "key": "text",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.text"
                        }
                      }
                    ]
                  },
                  {
                    "key": "time",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.time"
                        }
                      }
                    ]
                  },
                  {
                    "key": "author",
                    "valueExtraction": [
                      {
                        "source": {
                          "path": "$.authorString"
                        }
                      },
                      {
                        "source": {
                          "path": "$.authorReference"
                        },
                        "processor": "referenceText"
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ],
  "postProcessing": {
    "subclasses": [
      {
        "subclassId": "oncoContext",
        "filters": []
      }
    ]
  }
}