{
  "id": "imagingReports.onco.general",
  "items": [
    [
      {
        "key": "imagingReports",
        "query": {
          "from": "imagingReport",
          "filter": {
            "must": [
              {
                "date": {
                  "range": {
                    "lte": "now/d"
                  }
                }
              },
              {
                "attributes.status": {
                  "neq": "registered"
                }
              }
            ]
          },
          "dedup": true,
          "sort": [{ "date": "asc" }]
        }
      }
    ]
  ]
}
