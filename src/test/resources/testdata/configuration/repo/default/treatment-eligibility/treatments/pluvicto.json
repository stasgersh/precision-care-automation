{
  "id": "PLUVICTO",
  "type": "treatment",
  "studyCompletion": "2026-11",
  "name": "A prescription medicine used to treat men with PSMA+ metastatic castration-resistant prostate cancer (mCRPC).",
  "description": "PLUVICTO is a radioligand therapy (RLT). RLT is a different type of radiation therapy that is injected and that targets a biomarker called PSMA, which is present on many prostate cancer cells and also some healthy cells. This means it is designed to find and attack PSMA+ cells, including cancer cells.",
  "link": "https://us.pluvicto.com/support/faqs",
  "patientsFilter": {
    "filter": {
      "must": [
        {
          "attributes.gender": {
            "eq": "male"
          }
        }
      ]
    }
  },
  "variables": [
    {
      "id": "dob",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "PATIENT",
              "query": {
                "from": "patient",
                "show": [
                  "attributes.dob"
                ],
                "dedup": true
              }
            }
          ]
        }
      },
      "display": "Patient's date of birth",
      "usingAi": false
    },
    {
      "id": "gender",
      "definition": {
        "fulfillment": {
          "queries": [
            {
              "alias": "PATIENT",
              "query": {
                "from": "patient",
                "show": [
                  "attributes.gender"
                ],
                "dedup": true
              }
            }
          ]
        }
      },
      "display": "Patient's gender",
      "usingAi": false
    },
    {
      "id": "EcogStatus",
      "definition": {
        "ai": {
          "criteria": "Eastern Cooperative Oncology Group (ECOG) performance status (PS) < 3"
        }
      },
      "display": "EcogStatus"
    },
    {
      "id": "mCRPC",
      "definition": {
        "ai": {
          "criteria": "Diagnosis of metastatic castration-resistant prostate cancer (mCRPC)"
        }
      },
      "display": "mCRPC"
    },
    {
      "id": "PSMA",
      "definition": {
        "ai": {
          "criteria": "Disease is prostate-specific membrane antigen (PSMA)-positive, as confirmed on positive emission tomography (PET) or computed tomography (CT) scan"
        }
      },
      "display": "PSMA"
    },
    {
      "id": "Zytiga",
      "definition": {
        "ai": {
          "criteria": "Previous treatment with at least one androgen receptor-directed therapy, e.g., abiraterone (Zytiga®), enzalutamide (Xtandi®)"
        }
      },
      "display": "Zytiga"
    },
    {
      "id": "taxane",
      "definition": {
        "ai": {
          "criteria": "Previous treatment with taxane-based chemotherapy"
        }
      },
      "display": "taxane"
    },
    {
      "id": "CrCl",
      "definition": {
        "ai": {
          "criteria": "CrCl <= 29 mL/min "
        }
      },
      "display": "CrCl"
    },
    {
      "id": "ESRD",
      "definition": {
        "ai": {
          "criteria": " Patient has ESRD"
        }
      },
      "display": "ESRD"
    }
  ],
  "criteria": [
    {
      "id": "criteria_male",
      "type": "inclusion",
      "title": "Gender is male",
      "dependency": [
        "gender"
      ],
      "expression": "gender",
      "subCriteria": [
        {
          "id": "gender",
          "title": "Gender is male",
          "description": "Gender is male",
          "expression": "variables.gender.value[].attributes.gender|[0]== `male`",
          "explainability": {
            "displayValue": "variables.gender.value[].attributes.gender|[0]"
          }
        }
      ]
    },
    {
      "id": "criteria_age",
      "type": "inclusion",
      "title": "Age >= 18 years",
      "allowMissingVariables": [
        "dob"
      ],
      "dependency": [
        "dob"
      ],
      "expression": "isAgeGt18",
      "subCriteria": [
        {
          "id": "isAgeGt18",
          "title": "Age is greater than 18",
          "description": "Age is greater than 18",
          "expression": "variables.dob.value[].attributes.dob|abs_date_diff([0], `2025-03-10`,`years`,`int`) >= `18`",
          "explainability": {
            "displayValue": "variables.dob.value[].attributes.dob|abs_date_diff([0], `2025-03-10`,`years`,`int`)"
          }
        }
      ]
    },
    {
      "id": "EcogStatus",
      "type": "inclusion",
      "title": "Eastern Cooperative Oncology Group (ECOG) performance status (PS) < 3",
      "allowMissingVariables": [
        "EcogStatus"
      ],
      "dependency": [
        "EcogStatus"
      ],
      "expression": "EcogStatus",
      "subCriteria": [
        {
          "id": "EcogStatus",
          "title": "Eastern Cooperative Oncology Group (ECOG) performance status (PS) < 3",
          "description": "Eastern Cooperative Oncology Group (ECOG) performance status (PS) < 3",
          "expression": "variables.EcogStatus.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.EcogStatus.value.reasoning.join(' ', @)"
          }
        }
      ]
    },
    {
      "id": "mCRPC",
      "type": "inclusion",
      "title": "Diagnosis of metastatic castration-resistant prostate cancer (mCRPC)",
      "allowMissingVariables": [
        "mCRPC"
      ],
      "dependency": [
        "mCRPC"
      ],
      "expression": "mCRPC",
      "subCriteria": [
        {
          "id": "mCRPC",
          "title": "Diagnosis of metastatic castration-resistant prostate cancer (mCRPC)",
          "description": "Diagnosis of metastatic castration-resistant prostate cancer (mCRPC)",
          "expression": "variables.mCRPC.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.mCRPC.value.reasoning.join(' ', @)"
          }
        }
      ]
    },
    {
      "id": "criteria_imaging_dates",
      "type": "inclusion",
      "title": "Disease is prostate-specific membrane antigen (PSMA)-positive, as confirmed on positive emission tomography (PET) or computed tomography (CT) scan",
      "dependency": [
        "PSMA"
      ],
      "allowMissingVariables": [
        "PSMA"
      ],
      "expression": "PSMA",
      "subCriteria": [
        {
          "id": "PSMA",
          "title": "Disease is prostate-specific membrane antigen (PSMA)-positive, as confirmed on positive emission tomography (PET) or computed tomography (CT) scan",
          "description": "Disease is prostate-specific membrane antigen (PSMA)-positive, as confirmed on positive emission tomography (PET) or computed tomography (CT) scan",
          "expression": "variables.PSMA.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.PSMA.value.reasoning.join(' ', @)"
          }
        }
      ]
    },
    {
      "id": "criteria_PSMA",
      "type": "inclusion",
      "title": "Member meets ALL of the following",
      "dependency": [
        "Zytiga",
        "taxane"
      ],
      "expression": "Zytiga && taxane",
      "subCriteria": [
        {
          "id": "Zytiga",
          "title": "Previous treatment with at least one androgen receptor-directed therapy, e.g., abiraterone (Zytiga®), enzalutamide (Xtandi®)",
          "description": "Previous treatment with at least one androgen receptor-directed therapy, e.g., abiraterone (Zytiga®), enzalutamide (Xtandi®)",
          "expression": "variables.Zytiga.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.Zytiga.value.reasoning.join(' ', @)"
          }
        },
        {
          "id": "taxane",
          "title": "Previous treatment with taxane-based chemotherapy",
          "description": "Previous treatment with taxane-based chemotherapy",
          "expression": "variables.taxane.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.taxane.value.reasoning.join(' ', @)"
          }
        }
      ]
    },
    {
      "id": "CrCl_ESRD",
      "type": "exclusion",
      "title": "CrCl <= 29 mL/min or Patient has ESRD",
      "allowMissingVariables": [
        "CrCl",
        "ESRD"
      ],
      "dependency": [
        "CrCl",
        "ESRD"
      ],
      "expression": "CrCl || ESRD",
      "subCriteria": [
        {
          "id": "CrCl",
          "title": "CrCl <= 29 mL/min",
          "description": "CrCl <= 29 mL/min",
          "expression": "variables.CrCl.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.CrCl.value.reasoning.join(' ', @)"
          }
        },
        {
          "id": "ESRD",
          "title": "Patient has ESRD",
          "description": "Patient has ESRD",
          "expression": "variables.ESRD.value.response == `ELIGIBLE`",
          "explainability": {
            "displayValue": "variables.ESRD.value.reasoning.join(' ', @)"
          }
        }
      ]
    }
  ]
}